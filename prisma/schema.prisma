generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
  engineType = "client"
  runtime = "bun"
}

datasource db {
  provider = "mysql"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  membership Boolean @default(false)
}
